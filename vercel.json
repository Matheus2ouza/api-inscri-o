{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/python/processImages.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/img/(.*)", "dest": "/img/$1" }, 
    { "src": "/(.*)", "dest": "server.js" }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "static": {
    "directory": "img"
  }
}
